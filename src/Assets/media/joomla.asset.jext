{
	"$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
	"name": "{{prefix_component}}",
	"version": "{{version}}",
	"description": "{{description}}",
	"license": "{{license}}",
	"assets": [
		{
			"name": "{{prefix_component}}.icons",
			"type": "script",
			"uri": "{{prefix_component}}/icons.js",
			"dependencies": ["core"],
			"attributes": {
				"defer": true
			}
		},
		{
			"name": "{{prefix_component}}.icons",
			"type": "style",
			"uri": "{{prefix_component}}/icons.css"
		},
		{
			"name": "{{prefix_component}}.notes",
			"type": "style",
			"uri": "{{prefix_component}}/notes.css"
		}
	]
}
